+  channelId: number;
+}) => {
+  const { messageDelete } = useChannelContext();
+  const messagesEndRef = useRef<HTMLDivElement | null>(null);
+
+  useEffect(() => {
+    messagesEndRef.current?.scrollIntoView({
+      block: 'start',
+      behavior: 'smooth',
+    });
