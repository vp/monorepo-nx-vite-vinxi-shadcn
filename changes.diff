+alter table chat_app.users
+  enable row level security;
+alter table chat_app.channels
+  enable row level security;
+alter table chat_app.messages
+  enable row level security;
+alter table chat_app.user_roles
+  enable row level security;
+alter table chat_app.role_permissions
